pipeline{
    agent any
    stages{

        stage("Install Dependencies") {
            steps{
                sh "npm install"
            }
        }

        stage("Test")
        {
            steps{
                echo "tetsing done and tested wisely"
            }
        }

        stage("Deploying"){
            steps{
                sh 'npm pack'
            }
        }
    }
}
